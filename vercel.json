{
    "builds": [
      { "src": "index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  {
    "builds": [
      { "src": "build/**/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
    